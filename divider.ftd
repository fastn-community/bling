-- component heart-end:
boolean end-mark: false
optional caption title:
ftd.image-src divider-icon: $assets.files.static.divider-1.svg
optional ftd.image-src the-end: $assets.files.static.the-end-0.png

-- ftd.column:
margin-top.px: 24
margin-bottom.px: 24
width: auto
width if { ftd.device == "mobile" }: fill-container
align-self: center

-- ftd.image:
src: $heart-end.divider-icon
width: fill-container
height: auto

-- ftd.image:
if: { heart-end.end-mark }
src: $heart-end.the-end
width.fixed.px: 100
height: auto
align-self: center
margin-top.px: 24

-- ftd.text: $heart-end.title
if: { heart-end.title != NULL }
role: $inherited.types.copy-large
color: $inherited.colors.text
text-align: center
align-self: center
white-space: nowrap
margin-top.px: 16

-- end: ftd.column

-- end: heart-end










